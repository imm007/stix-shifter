{
  "domain-name": {
    "fields": {
      "value": ["host"]
    }
  },
  "x-datadog-event": {
    "fields": {
      "event_id": ["id"],
      "priority": ["priority"],
      "title": ["title"],
      "text": ["text"],
      "device_name": ["device_name"],
      "source": ["source"],
      "monitor_id": ["monitor_id"],
      "tags": ["tags"],
      "is_aggregate": ["unaggregated"],
      "alert_type": ["alert_type"]
    }
  },
  "x-ibm-finding": {
    "fields": {
      "start": ["start"],
      "end": ["end"],
      "time_observed": ["date_happened"]
    }
  },
  "x-oca-event": {
    "fields": {
      "code": ["id", "id_str"],
      "outcome": ["title"],
      "original.payload_bin": ["text"]
    }
  }
}