{
  "ipv4-addr": {
    "fields": {
      "value": ["IpAddress"]
    }
  },
  "directory": {
    "fields": {
      "path": ["HomeDirectory", "HomePath"]
    }
  },
  "file": {
    "fields": {
      "path": ["FilePath"],
      "hashes": ["FileHash"]
    }
  },
   "process" : {
    "fields": {
      "name": ["ProcessName", "LogonProcessName"],
      "parent_ref.name": ["ParentProcessName"],
      "command_line": ["CommandLine"],
      "pid": ["ProcessId"]
    }
  },
  "domain-name": {
    "fields": {
      "value": ["AccountDomain", "DomainName", "SubjectDomainName", "TargetDomainName", "TargetOutboundDomainName"]
    }
  },
  "url": {
    "fields": {
      "name": [ "QuarantineHelpURL"]
    }
  },
  "x-ibm-finding": {
    "fields": {
      "start": ["TimeGenerated"],
      "end": ["PreviousTime"],
      "code": ["eventID"],
      "name": ["EventSourceName"],
      "time_observed": ["TimeGenerated"],
      "src_ip_ref": ["IpAddress"],
      "dst_ip_ref": ["ClientIPAddress"]
    }
  },
  "x-oca-event": {
    "fields": {
      "created": ["TimeGenerated"],
      "code": ["eventID"],
      "provider": ["AccountName"],
      "url_ref": ["QuarantineHelpURL"],
      "process_ref": ["ProcessName"],
      "file_ref.hash": ["FileHash"],
      "file_ref.path": ["FilePath"],
      "file_ref.service": ["ServiceFileName"],
      "parent_process_ref": ["ParentProcessName"],
      "domain_ref": ["DomainName"],
      "ip_refs.ip": ["IpAddress"],
      "ip_refs.clientIp": ["ClientIPAddress"]
    }
  },
  "x-msazure-sentinel-event": {
    "fields": {
      "event_id": ["eventID"],
      "title": ["Title"],
      "text": ["Description", "DeviceDescription"],
      "device_name": ["DeviceId"],
      "source": ["SourceSystem", "EventSourceName"],
      "alert_type": ["AlertName"],
      "account": ["Account"],
      "activity": ["Activity"],
      "computer": ["Computer"],
      "logon_processname": ["LogonProcessName"],
      "event_data": ["EventData"],
      "service_name": ["ServiceName"],
      "task": ["Task"],
      "user_parameter": ["UserParameter"],
      "member_name": ["MemberName"],
      "requester": ["Requester"]
    }
  }
}
